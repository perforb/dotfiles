# Customize a PATH
# See `/etc/paths`
source /etc/profile

# rbenv
eval "$(rbenv init -)"

# alias
alias xargs="gxargs"

# source-highlight
export LESS=mqeisz-2XR
export LESSOPEN='| /usr/local/bin/src-hilite-lesspipe.sh %s'

# editor
export EDITOR=emacsclient

# z
# brew install z
# https://github.com/rupa/z
. `brew --prefix`/etc/profile.d/z.sh
